<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="FeiQiMapper">
	<insert id="save" parameterType="pd">
		insert into feiqi(
			USER_ID,
			USERNAME,
			IMG,
			DATE
		)values(
			#{USER_ID},
			#{USERNAME},
			#{IMG},
			#{DATE}
		)
	</insert>
	
</mapper>